  PACKED_PRIVATE_KEY_1 = dec64 ''
  PACKED_PRIVATE_KEY_2 = dec64 ''

  CONTRACT_KEY = dec64 """
    
  """

  CONTRACT_BIN = dec64 """
    
  """

  U_CONTRACT_BIN = dec64 """
    
  """

  U_CONTRACT_BIN2 = dec64 """
    
  """

  CONTRACT_BIN2 = dec64 """
    
  """

  U_KEY = dec64 """
    
  """

  APP_TOKEN = "ehUEdnfhDqhBAWK77Rndam0d+L6NVhGDGm/ma3SHHZQA4kgs4uf1AnYJKqKHU2qQZchSeieZyquGOuCHeUtULw=="

  Unitest.addTest 'check Capsule.getInfo', ->
    contract = Contract.fromBOSS(CONTRACT_BIN2)
    result = contract.temp.getInfo()

    console.log(result)
    @assertEquals result.publicKeys.length, 1
    @assertEquals result.roles.length, 3

    pubKey = result.publicKeys[0]

    @assertTrue contract.temp.isSignedBy(pubKey)

    privKey = Universa.PrivateKey.fromBOSS(CONTRACT_KEY)
    pubKey2 = privKey.publicKey
    @assertFalse contract.temp.isSignedBy(pubKey2)

  Unitest.addTest 'check Capsule.getPermissions', ->
    privKey = Universa.PrivateKey.fromBOSS(CONTRACT_KEY)
    pubKey = privKey.publicKey

    contract = Contract.createToken(pubKey, '100', 0.1, "My test token", "MTK", "description")
    result = contract.temp.getPermissions()
    console.log(result)

    contract = Contract.createToken(pubKey, '100', 0.1, "My test token", "MTK", "description", true)
    result = contract.temp.getPermissions()
    console.log(result)

  Unitest.addTest 'check isRoleAvailable method', ->
    privKey = Universa.PrivateKey.fromBOSS(CONTRACT_KEY)
    pubKey = privKey.publicKey

    contract = Contract.createToken(pubKey, '100', 0.1, "My test token", "MTK", "description", true)
    roleAvailabilityResult = contract.temp.isRoleAvailable('owner', [pubKey])
    @assertTrue roleAvailabilityResult

    roleAvailabilityResult = contract.temp.isRoleAvailable('another role', [pubKey])
    @assertFalse roleAvailabilityResult

    roleAvailabilityResult = contract.temp.isRoleAvailable('another role', [])
    @assertFalse roleAvailabilityResult

